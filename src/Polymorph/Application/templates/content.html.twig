
{% extends baseTemplate %}

{% block imports %}

    {{ parent() }}
    <link rel="import" href="bower_components/polymorph/src/Polymorph/Page/elements/polymorph-page.html"/>

{% endblock %}

{% block view %}

    <polymorph-page
            class="polymorph-partial"
            path="{{ view.path }}"
            element="{{ element }}"
            element-data="{{ elementData|json_encode }}">
        {{ content|raw }}
    </polymorph-page>

{% endblock %}
