{
  "name": "polymorph",
  "version": "0.1.11",
  "homepage": "https://github.com/bnowack/polymorph",
  "description": "A PHP micro-framework for creating Polymer applications that are both browser- and bot-friendly",
  "authors": [
    "Benjamin Nowack <mail@bnowack.de>"
  ],
  "keywords": [
    "polymer",
    "php",
    "webcomponents"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bnowack/polymorph.git"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "dev",
    "vendor",
    "composer.*",
    "**/*.php",
    "**/*.twig"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.*",
    "app-layout": "polymerelements/app-layout#^0.*",
    "app-route": "polymerelements/app-route#^0.*",
    "iron-pages": "polymerelements/iron-pages#^1.0.0",
    "iron-selector": "polymerelements/iron-selector#^1.0.0",
    "iron-ajax": "PolymerElements/iron-ajax#^1.4.3",
    "paper-card": "PolymerElements/paper-card#^1.1.3",
    "paper-input": "^1.1.20",
    "paper-button": "^1.0.13",
    "iron-collapse": "PolymerElements/iron-collapse#^1.2.1"
  },
  "devDependencies": {
    "living-cukes": "bnowack/living-cukes#*"
  }
}
